{
  "expo": {
    "name": "ToDoList",
    "slug": "mohit_todolist",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/entry.png",
      "resizeMode": "contain",
      "backgroundColor": "#093685"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/entry.png",
        "backgroundColor": "#093685"
      },
      "package":"com.mohit_todolist.myapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
